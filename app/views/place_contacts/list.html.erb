<h1>Listing place_contacts</h1>

<table>
  <tr>
  <% for column in PlaceContact.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for place_contact in @place_contacts %>
  <tr>
  <% for column in PlaceContact.content_columns %>
    <td><%=h place_contact.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => place_contact %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => place_contact %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => place_contact }, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @place_contact_pages.current.previous } if @place_contact_pages.current.previous %>
<%= link_to 'Next page', { :page => @place_contact_pages.current.next } if @place_contact_pages.current.next %> 

<br />

<%= link_to 'New place_contact', :action => 'new' %>
