<h1>Listing quiz_types</h1>

<table>
  <tr>
  <% for column in QuizType.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for quiz_type in @quiz_types %>
  <tr>
  <% for column in QuizType.content_columns %>
    <td><%=h quiz_type.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => quiz_type %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => quiz_type %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => quiz_type }, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @quiz_type_pages.current.previous } if @quiz_type_pages.current.previous %>
<%= link_to 'Next page', { :page => @quiz_type_pages.current.next } if @quiz_type_pages.current.next %> 

<br />

<%= link_to 'New quiz_type', :action => 'new' %>
