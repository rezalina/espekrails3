<!--script-->
  new Highcharts.Chart({
    chart: {
      renderTo: '<%= container %>',
      type: 'bar'
    },
    title: {
      text: '<%= title %>'
    },
    xAxis: {
      categories: <%= category %>,
      title: {
        text: null
      }
    },
    yAxis: {
      min: 0,
      title: {
        text: 'No. of Patents',
        align: 'high'
      },
      labels: {
        overflow: 'justify'
      }
    },
    plotOptions: {
      bar: {
        colorByPoint: true,
        dataLabels: {
          enabled: true
        }
      }
    },
    legend: {
      layout: 'vertical',
      align: 'right',
      verticalAlign: 'bottom',
      x: -30,
      y: -60,
      floating: true,
      borderWidth: 1,
      backgroundColor: '#FFFFFF',
      shadow: true
    },
    credits: {
      position: {
        align: 'left',
        x: 20 },
      href: 'http://www.instun.gov.my',
      text: 'Kementrian Pendidikan Kebudayaan Malaysia'
    },
    series: <%= data %>
  });
<!--/script-->