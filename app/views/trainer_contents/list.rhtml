<h1>Listing trainer_contents</h1>

<table>
  <tr>
  <% for column in TrainerContent.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for trainer_content in @trainer_contents %>
  <tr>
  <% for column in TrainerContent.content_columns %>
    <td><%=h trainer_content.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => trainer_content %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => trainer_content %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => trainer_content }, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @trainer_content_pages.current.previous } if @trainer_content_pages.current.previous %>
<%= link_to 'Next page', { :page => @trainer_content_pages.current.next } if @trainer_content_pages.current.next %> 

<br />

<%= link_to 'New trainer_content', :action => 'new' %>
