<h1>Listing quiz_objectives</h1>

<table>
  <tr>
  <% for column in QuizObjective.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for quiz_objective in @quiz_objectives %>
  <tr>
  <% for column in QuizObjective.content_columns %>
    <td><%=h quiz_objective.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => quiz_objective %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => quiz_objective %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => quiz_objective }, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @quiz_objective_pages.current.previous } if @quiz_objective_pages.current.previous %>
<%= link_to 'Next page', { :page => @quiz_objective_pages.current.next } if @quiz_objective_pages.current.next %> 

<br />

<%= link_to 'New quiz_objective', :action => 'new' %>
