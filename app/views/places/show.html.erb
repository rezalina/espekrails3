<table border="0" cellspacing="0" cellpadding="0" width="95%" align="center">
    <tr> 
      <td width="86%" height="21" align="center" bgcolor="#999999"><b><font color="#FFFFFF">MAKLUMAT JABATAN</font></b></td>
    </tr>
  </table>
  <br>
   <table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
      <td align="center"> <a href="/places/list">Kembali ke senarai Jabatan</a></td>
    </tr>
  </table>
 <br>
  <table width="90%" border="0" align="center" cellpadding="3" cellspacing="1" bgcolor="#eeeeee">
    <tr>
      <td align="right"><strong>Kementerian:</strong></td>
      <td><span class="blockintro">&nbsp; <%= nof{@place.parent.name} %> </span></td>
    </tr>
    <tr>
      <td width="40%" align="right"><strong>Kod Jabatan: </strong></td>
      <td><span class="blockintro">
        &nbsp;
      <%= nof{@place.code} %>      </span></td>
    </tr>
     <tr>
      <td align="right"><strong>Gelaran Jawatan Tertinggi: </strong></td>
      <td width="60%">&nbsp;
      <%= nof{@place.head_title}  %></td>
    </tr>
    <tr>
      <td align="right"><strong>Nama Jabatan:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= nof{@place.name}  %></td>
    </tr>
    <tr>
      <td align="right" valign="top"><strong>Alamat:</strong></td>
      <td><span class="blockintro">&nbsp;
      <%= nof{@place.send('address1')}  %>      </span></td>
    </tr>
    <tr>
      <td align="right">&nbsp;</td>
      <td><span class="blockintro">&nbsp;
      <%= @place.send('address2')  %>      </span></td>
    </tr>
    <tr>
      <td align="right">&nbsp;</td>
      <td><span class="blockintro">&nbsp;
      <%= @place.send('address3')  %>      </span></td>
    </tr>
    <!--tr>
      <td align="right"><strong>Daerah:</strong></td>
      <td>&nbsp;
      <%= nof{@place.district.description} %>      </td>
    </tr-->
    <tr>
      <td align="right"><strong>Negeri:</strong></td>
      <td>&nbsp;
      <%= @place.state.description if @place.state %>      </td>
    </tr>
    <tr>
      <td align="right"><strong>No Telefon:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= @place.phone  %></td>
    </tr>
     <tr>
      <td align="right"><strong>No Faks:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= @place.fax %></td>
    </tr>
      <tr>
        <td colspan="2" align="CENTER" >&nbsp;</td>
      </tr>
      <tr>
      <td colspan="2" align="CENTER" ><strong><u>Pegawai Untuk Dihubungi</u>&nbsp;&nbsp;</strong></td>
    </tr>
    <tr>
      <td align="right"><strong>Nama:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= nof{@place_contact.name}  %></td>
    </tr>
     <tr>
      <td align="right"><strong>Jawatan:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= nof{@place_contact.position} %></td>
    </tr>
	<tr>
      <td align="right"><strong>No Telefon:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= nof{@place_contact.phone}  %></td>
    </tr>
     <tr>
      <td align="right"><strong>Emel:</strong></td>
      <td><span class="blockintro">&nbsp; </span><%= nof{@place_contact.email} %></td>
    </tr>
  </table>
