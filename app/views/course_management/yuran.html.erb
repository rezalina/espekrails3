<script>
  function GoSubmit(df) {
    return true;
  }

  function daftarPeserta(df) {
    df.action = "/course_management/make_payment"
    df.submit()
  }

  function printPeserta(df) {
    df.action = "/course_management/cetak_yuran.pdf"
    df.submit()
  }

  function checkAll(df) {
    for (i=1; i<df.elements.length; i++ )
    {
      if (df.elements[i].type == "checkbox")
      {
        df.elements[i].checked = df.check_all.checked
      }
    }
  }


</script>

