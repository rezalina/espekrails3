    //"<li><a href=\"" + "/timetables" + "/list/" + objid + "\">&nbsp;&nbsp;Jadual Waktu</a>" +
    "<hr size=1>" +
      "<li><a href=\"" + "/course_applications" + "/all/" + objid + "\">&nbsp;&nbsp;Lihat Senarai Peserta</a>" +
