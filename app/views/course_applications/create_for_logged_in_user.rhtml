<div align="center">
  <input type="button" name="Submit" value="Keluar" onclick="location='/course_implementations/list_courses_from_today_to_future'">
  <input type="button" name="Submit2" value="Cetak" onclick="CetakForLoggedInUser(<%=@course_application.id%>)">
</div>

<script>
function CetakForLoggedInUser(id) {
	window.open('/course_applications/cetak_for_logged_in_user/'+id,'_blank','scrollbars=yes,width=700,height=450')
}
</script>